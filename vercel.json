{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    
      { "src": "/api/auth", "dest": "api/auth.js" },
      { "src": "/api/users", "dest": "api/users.js" },
      { "src": "/api/tasks", "dest": "api/posts.js" },
      { "src": "/api/uploads", "dest": "api/uploads.js" }
    
  ]

}